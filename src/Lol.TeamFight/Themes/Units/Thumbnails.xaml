<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:units="clr-namespace:Lol.TeamFight.UI.Units">

	<Style TargetType="{x:Type units:Thumbnails}">
		<Setter Property="Grid.Row" Value="2"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type units:Thumbnails}">
					<Border Background="#AA000000">
						<Grid Margin="20 15 20 15">
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="1*"/>
								<ColumnDefinition Width="1*"/>
								<ColumnDefinition Width="1*"/>
								<ColumnDefinition Width="1*"/>
							</Grid.ColumnDefinitions>
                            <units:ThumbnailButton Grid.Column="0" Order="1" Content="10.25 전략적 팀 전투 패치 노트"/>
							<units:ThumbnailButton Grid.Column="1" Order="2" Content="야수의 축제"/>
							<units:ThumbnailButton Grid.Column="2" Order="3" Content="K/DA 꼬마 전설이"/>
							<units:ThumbnailButton Grid.Column="3" Order="4" Content="별 파편으로 등급을 올리세요"/>
						</Grid>
					</Border>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

</ResourceDictionary>