<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:Lol.Clash.UI.Views"
                    xmlns:units="clr-namespace:Lol.Clash.UI.Units"
                    xmlns:ctrls="clr-namespace:DevNcore.LayoutSupport.Leagueoflegends.Controls;assembly=DevNcore.LayoutSupport.Leagueoflegends">
	<Style TargetType="{x:Type views:WinnersView}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type views:WinnersView}">
					<Grid Margin="0 16 0 0">
						<ctrls:RiotTextBox MinWidth="220" 
										   Height="30" 
										   HorizontalAlignment="Right" 
										   VerticalAlignment="Top" 
										   PlaceHolder="팀 검색" 
										   Margin="0 60 40 0"/>
                        <units:TierDetailListBox/>
						<units:TierMenu/>
						<units:Title/>
                    </Grid>
				</ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>