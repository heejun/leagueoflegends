<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:units="clr-namespace:Lol.TeamFight.UI.Units">
	
	<Style TargetType="{x:Type units:FatesPass}">
		<Setter Property="Grid.Column" Value="0"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type units:FatesPass}">
					<StackPanel>
						<units:TitleControl/>
						<units:SubTitleControl/>
						<units:UpgradeButton/>
						<units:LevelControl/>
					</StackPanel>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
</ResourceDictionary>